function hideLogo() {
    const scroll = document.getElementById("logo");
    if (document.body.scrollTop > 100 || scroll.scrollTop > 100) {

        document.getElementById("logo").className = "hideLogo";



    } else {
        document.getElementById("logo").className = "logo";

    }
}

